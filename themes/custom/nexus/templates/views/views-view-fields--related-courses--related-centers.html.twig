    <div class="map-box">
        <div class="detailbox">
            <h2>{{ fields.title.content|striptags|trim }}</h2>
            <p>{{ fields.field_center_location.content|striptags|trim }}</p>
            <p>
                <strong>Phone:</strong>{{ fields.field_center_phone.content|striptags|trim }}
            </p>
            <p>
                <strong>E-mail:</strong> {{ fields.field_center_email.content|striptags|trim }}
