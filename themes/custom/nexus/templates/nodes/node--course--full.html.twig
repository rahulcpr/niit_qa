<div id="courseDemoModal" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <h2 class="modal-title">Course Demo</h2>
            </div>
            <div class="modal-body iframemodal">
                <button type="button" class="close" data-dismiss="modal"
                    onclick="stopVideo()">&times;
                </button>
                <div class="embed-responsive embed-responsive-16by9">
                    <iframe class="embed-responsive-item" id="demoVideoIframe"
                        width="100%" height="315" frameborder="0"
                        allow="encrypted-media"
                        allowfullscreen></iframe>
                </div>
                <script type="text/javascript">
                    var url = "{{ node.field_course_demo_video.value }}";
                    var id = url.split("?v=")[1];

                    var embedlink = "https://www.youtube.com/embed/" + id;

                    var demoYoutube = document.getElementById("demoVideoIframe");

                    demoYoutube.src = embedlink;

                    function stopVideo() {
                        demoYoutube.src = embedlink;
                    }
                </script>
            </div>
            <div class="modal-footer text-center">
                <button type="button" class="btn btn-default"
                    data-dismiss="modal" onclick="stopVideo()">Close
                </button>
            </div>
        </div>
    </div>
</div>
