<!-- Course Overview end--> 

<section class="new_prodcutpage_usp">
  <div class="container">
    <div class="row">
      <div class="product_usps">
        <div class="col-md-9 new_usp_box">

          <div class="row">
            {% if node.field_course_duration.value is not empty %}
            <div class="col-md-4 col-xs-6">
              <div class="media">
                <div class="media-left">
                  <img class="media-object" src="{{ file_url(node.field_course_duration_icon.entity.fileuri) }}" alt="calender">
                </div>
                <div class="media-body">
                  <h4 class="media-heading"><strong>{{ node.field_course_duration.value }} </strong></h4>
                  <p>{{ node.field_course_duration_tooltip.value }} </p>
                </div>
              </div>
            </div>
            {% endif %}
            {% if node.field_mode_of_delivery.value is not empty %}
            <div class="col-md-4 col-xs-6">
              <div class="media">
                <div class="media-left">
                  <img class="media-object" src="/india/themes/custom/nexus/assets/images/vid.png" alt="calender">
                </div>
                <div class="media-body">
                  <h4 class="media-heading pr_10"><strong>{{ node.field_mode_of_delivery.value }}</strong></h4>
                </div>
              </div>
            </div>
            {% endif %}
            {% if node.field_course_modules.value is not empty %}
            <div class="col-md-4 col-xs-6">
              <div class="media">
                <div class="media-left">
                  <img class="media-object" src="{{ file_url(node.field_course_module_icon.entity.fileuri) }}" alt="calender">
                </div>
                <div class="media-body">
                  <h4 class="media-heading pr_10"><strong>{{ node.field_course_modules.value }}</strong></h4>
                </div>
              </div>
            </div>
            {% endif %}
            {% if node.field_course_type.value is not empty %}
            <div class="col-md-4 col-xs-6">
              <div class="media">
                <div class="media-left">
                  <img class="media-object" src="{{ file_url(node.field_course_type_icon.entity.fileuri) }}" alt="calender">
                </div>
                <div class="media-body">
                  <h4 class="media-heading"><strong>{{ node.field_course_type.value }}</strong></h4>
                  <p> {{ node.field_course_type_tooltip.value }}</p>
                </div>
              </div>
            </div>
            {% endif %}
            {% if node.field_rated_text.value is not empty %}
            <div class="col-md-4 col-xs-6">
              <div class="media">
                <div class="media-left">
                  <img class="media-object" src="{{ file_url(node.field_rated_icon.entity.fileuri) }}" alt="calender">
                </div>
                <div class="media-body">
                  <h4 class="media-heading"><strong>{{ node.field_rated_text.value }}</strong></h4>
                  <p>{{ node.field_rated_tooltip.value }}</p>
                </div>
              </div>
            </div>
            {% endif %}
            {% if node.field_course_student_enrolled.value is not empty %}
            <div class="col-md-4 col-xs-6">
              <div class="media">
                <div class="media-left">
                  <img class="media-object" src="{{ file_url(node.field_student_enrolled_icon.entity.fileuri) }}" alt="calender">
                </div>
                <div class="media-body">
                  <h4 class="media-heading"><strong>{{ node.field_course_student_enrolled.value }}</strong></h4>
                  <p>{{ node.field_student_enrolled_tooltip.value }}</p>
                </div>
              </div>
            </div>
            {% endif %}
          </div> 

        </div>
        <div class="col-md-3 col-xs-12 timer_box">
          <div class="col-md-12 col-xs-7">
            {% if node.field_course_batch_start_text.value is not empty %}
              <p class="pt-4 mb-0">{{ node.field_course_batch_start_text.value }}</p>
            {% else %}
            <p class="pt-4 mb-0">Batch Start Date</p>
            {% endif %}
            <h4 class="mt-0 mb-4">{{ courseBatchStartDate }}</h4>
          </div>
          {{ courseBatchStartDateTimeCounter|raw }}
        </div>
      </div>
    </div>
	<p class="mt-3 pl-3"><i>*Terms and Conditions Apply. Refer FAQs.</i></p>
  </div>
</section>

