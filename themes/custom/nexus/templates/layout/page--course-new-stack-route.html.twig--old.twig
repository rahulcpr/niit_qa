{% if course_page_type == "course_new_stack_route" %}
  <!-- Header Start-->
<header role="banner">
    {% include directory ~ '/templates/layout/header.html.twig' %}
</header>
<!-- Header End -->

  
{% if node.field_banner_content.value is not empty %}
  {% set new_main_class = "digital-marketing-page" %}
{% else %}
  {% set new_main_class = "" %}
{% endif %}

<section id="stackroute-pageMainSection" class="stackroute-pageMainSection {{ new_main_class }}">
<!-- Breadcrub Start -->
<section class="course-page-breadcrub mobile-hide icici-breadcrumb">
  <div class="container">
    <div class="row">
            <div class="col-lg-9">
                <div class="page-breadcrumbs">
                    <ul class="breadcrumb">
                        <li>
                            <a href="/">Home</a>
                        </li>
                        <li>
                            <a id="courseCategory"
                                href="{{ path('entity.node.canonical', {'node':  node.field_top_course_category.entity.id}) }}">{{ node.field_top_course_category[0].entity.title.value }}
                            </a> 
                        </li>
                        <li>{{ node.label }}</li> 
                    </ul>
                </div>
            </div>
            <div class="col-lg-3 desk-com">
              {# {{ complete_app_btn|raw }} #}
              <div id="continue-reg-pop"></div>

            </div>
      </div>
  </div>
</section>
<div id="popmulti-div"></div>
<div id="popmulti-div-free"></div>
<!-- Breadcrub End -->
<!-- nav bar stricky Start-->
<div id="icici-page-navbar">
  <div id="icici-navbar">
    <div class="container">
      <div class="row desktop-hide icici-page-scroll">
        <ul class="nav nav-tabs" role="tablist">
          {% if node.field_floater_menu is not empty %}
            {% for key, value in node.field_floater_menu %}
              <li><a href="#{{ value.entity.field_menu_section.value }}" class="searchbycharICICI">{{ value.entity.field_menu_title.value }}</a></li>
            {% endfor %}
          {% endif %}
          {% if node.field_click_to_call_phone_number.value is not empty %}
            <li><a href="tel:{{ node.field_click_to_call_phone_number.value }}"><i class="fa fa-phone"></i>{{ node.field_click_to_call_phone_number.value }}</a></li>
          {% endif %}
          <li class="new_mutlistep_apply_btn new_stackroute_apply_btn newbtnApplypink">{{ mutlistep_apply_btn|raw }} </li>
        </ul>
      </div>
      <div class="row pl-0 mobile-hide">
        <div class="col-md-8 icici-page-scroll">
          {% if node.field_floater_menu is not empty %}
            {% for key, value in node.field_floater_menu %}
              <a href="#{{ value.entity.field_menu_section.value }}" class="searchbycharICICI">{{ value.entity.field_menu_title.value }}</a>
            {% endfor %}
          {% endif %}
        </div>
        <div class="col-md-4 newstackroute-navbar-btn">
          <p class="iciciStickyNav new_mutlistep_apply_btn newbtnApplypink new_stackroute_apply_btn">
            {% if node.field_click_to_call_phone_number.value is not empty %}
              <a href="tel:{{ node.field_click_to_call_phone_number.value }}"><i class="fa fa-phone"></i>{{ node.field_click_to_call_phone_number.value }}</a>
            {% endif %}
            {{ mutlistep_apply_btn|raw }} </p>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- nav bar stricky end-->


<!-- Course Overview start-->

<section class="newImgBanner stackRouteNewBanner new_product_page">
