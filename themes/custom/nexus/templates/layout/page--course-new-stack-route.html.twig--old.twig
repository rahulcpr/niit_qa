          <source class="img-responsive banner-img js-lazyload-image desktop-hide" data-srcset="{{ webp_image_get(node.field_mobile_banner_image.entity.fileuri |image_style('mob_banner')) }}" type="image/webp" srcset="/india/sites/default/files/transparentimg.jpg" alt="{{ node.field_mobile_banner_image.alt  }}">
