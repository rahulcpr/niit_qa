                <span class="video_play_title">Watch <br><span>Program Video</span></span>
              </p>
              {% endif %}
              {% endif %}
             
              {# {% if node.field_free_trial.value == 1 %}
              <div class="col-md-5 col-xs-6 mt-3 new_mutlistep_apply_btn new_stackroute_apply_btn new_stackroute_apply_btn_fcopy mobile-hide">
              {{ free_trial_apply_btn|raw }} 
              </div>
              {% endif %} #}

              
               
              {% if node.field_free_trial.value == 1 %}
              {% if node.field_course_video_review[0].input is not empty %}
              <p class="col-md-4 col-xs-6 newIntroVideo" onclick="video_iframe_popup('{{ node.field_course_video_review[0].input }}')" >
                <span class="video-play-btn"><span></span></span>
                <!--<img src="/india/themes/custom/nexus/assets/images/video-icon.png" class="img-responsive iciciPlayBtn">-->
                <span class="video_play_title">Watch <br><span>Program Video</span></span>
              </p>
              {% endif %}
              {% endif %}

              
               
                

            </div>
              
               

               
               
                

            
            
              {% if node.field_banner_content.value is not empty %}
                <div class="row mt-5">
                  <div class="col-md-12">
                    {{ node.field_banner_content.value|raw }}
                  </div>
                </div>
              {% endif %}
            
              {# <span class="desktop-hide">
                {% if node.field_course_video_review[0].input is not empty %}
                <p class="col-md-4 newIntroVideo" data-toggle="modal" data-target="#myModal-videos">
                  <img data-src="/india/themes/custom/nexus/assets/images/video-icon.png" class="img-responsive js-lazyload-image" src="/india/sites/default/files/transparentimg.jpg">
                  <span>Watch <br><span>pogramme video</span></span>
                </p>
                {% endif %} 
              </span> #}
          </div>
        </div>
        {# <div class="col-md-1 col-md-offset-1">
          <div class="newStackRouteLogo">
            {% if node.field_title_logo.entity.fileuri is not empty %}
