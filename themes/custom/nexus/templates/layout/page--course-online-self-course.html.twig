<section class="productPageBanner mobile-hide {{ CourseTemplateCategory }}">
   <div class="container">
      <div class="row">
         <div class="col-md-3">
                <div class="card-body card-body-cascade text-center video-cardSlider"> 
               <div id="myCarousel55" class="carousel slide" data-ride="carousel">
                   <!-- Indicators -->
                     <ol class="carousel-indicators">
                       {{ careerSliderDotDiv|raw }}
                     </ol>
                   <!-- Wrapper for slides -->
                     <div class="carousel-inner">
                         {{ career_imageVideo_div|raw }}                    
                     </div>               
                </div>
               {{ drupal_view('course_testimonials', 'page_course_brochure', course_post_id ) }}
