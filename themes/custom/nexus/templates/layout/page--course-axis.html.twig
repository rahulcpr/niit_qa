      <picture>
        <source class="img-responsive banner-img js-lazyload-image" data-srcset="{{ webp_image_get(node.field_background_video_image.entity.fileuri | image_style('1000')) }}" type="image/webp" srcset="/india/sites/default/files/transparentimg.jpg" alt="{{ node.field_background_video_image.alt }}"/>
        <img class="img-responsive banner-img js-lazyload-image" data-src="{{ node.field_background_video_image.entity.fileuri }}" src="/india/sites/default/files/transparentimg.jpg" alt="{{ node.field_background_video_image.alt  }}" />
      </picture>
      {% endif %}

<section class="newBannerStyle">  
    <div class="container">
      <div class="newBannerTextOverlay">
        <div class="col-md-9 pl-0 pr-0">
          <div class="newIciciLogo">
            {% if node.field_title_logo.entity.fileuri is not empty %}
              <!--<img class="img-responsive iciciLogo js-lazyload-image" src="/india/sites/default/files/transparentimg.jpg" data-src = "{{ file_url(node.field_title_logo.entity.fileuri) }}" alt="{{ node.field_title_logo.alt  }}" />-->
              <picture>
                <source class="img-responsive iciciLogo js-lazyload-image" data-srcset="{{ webp_image_get(node.field_title_logo.entity.fileuri |image_style('1000')) }}" type="image/webp" srcset="/india/sites/default/files/transparentimg.jpg" alt="{{ node.field_title_logo.alt }}"/>
                <img class="img-responsive iciciLogo js-lazyload-image" data-src="{{ node.field_title_logo.entity.fileuri }}" src="/india/sites/default/files/transparentimg.jpg" alt="{{ node.field_title_logo.alt  }}"/>
              </picture>
              {% endif %}
          </div>
          <div class="offer_ticker mt-5">
              {% if node.field_placement_offer.value is not empty %}
              {% if node.field_placement_offer_text.value is not empty %}
                <span class="ticker_title">{{ node.field_placement_offer.value }}</span>
                <div class="programme_ticker">
                <div class="ticker_text_animate"><marquee>{{ node.field_placement_offer_text.value }}</marquee></div>
                </div>
                {% endif %}
                {% endif %}
              </div>
          <div class="newCourseDetail-1">
            <h1 class="mt-2">{{ node.title.value }}</h1>
            {% if node.field_course_summary.value is not empty %}
              <h6>{{ node.field_course_summary.value }}</h6>
            {% endif %}
            {% if node.field_course_batch.value is not empty %}
              <p class="batch_start"><i class="fa fa-calendar-check-o"></i> {{ node.field_course_batch.value }}</p>
            {% endif %}
            {# <h5 class="newRating">
              {% if CourseStarRating is not empty %}
                {{ CourseStarRating|raw }}
              {% endif %}
              {% if CourseReview is not empty %}
                <span>{{ CourseReview }}</span>
              {% endif %}
            </h5> #}
            <p class="vide0Card-share-icons share-icici new-share-icon-modular">Share
              <a target="_blank" href="https://www.facebook.com/sharer/sharer.php?u={{ request_protocol_domain ~ request_path_uri }}" rel="nofollow"><i class="fa fa-facebook"></i></a>

              <a target="_blank" href="https://twitter.com/intent/tweet?url={{ request_protocol_domain ~ request_path_uri }}" rel="nofollow"><i class="fa fa-twitter"></i></a>

              <a target="_blank"  href="https://www.linkedin.com/cws/share?url={{ request_protocol_domain ~ request_path_uri }}&token=&isFramed=true" rel="nofollow"><i class="fa fa-linkedin"></i></a>

            </p>
            {% if node.field_course_video_review[0].input is not empty %} 
            <div class="col-md-4 col-xs-5 intro-video-pop">
              <p class="newIntroVideo" onclick="video_iframe_popup('{{ node.field_course_video_review[0].input }}')" >
                <span class="video-play-btn"><span></span></span>
                {# <img src="/india/themes/custom/nexus/assets/images/video-icon.png" class="img-responsive iciciPlayBtn"> #}
                <span>Watch <br><span>INTRO VIDEO</span></span>
              </p>
            </div>
            {% endif %}    
            <div class="col-md-3 col-xs-5 pl-0 pr-0">
              {% if node.field_mode_of_delivery.value is not empty %} 
              <div class="modeOfdelivery">
                <h5>Mode Of Delivery</h5>
                <p>{{node.field_mode_of_delivery.value}}
                  {% if node.field_mode_of_delivery_tooltip.value is not empty %}
                    <small class="FeeTooltip deliveryModeInfo">
                      <i class="fa fa-info-circle"></i>
                      <small class="tip_popup">{{ node.field_mode_of_delivery_tooltip.value }}</small>
                    </small>
                  {% endif %}
                </p>
              </div>
              {% endif %}
              {# System Requirement  Start #}
                {% if node.field_system_requirement_title.value is not empty %}
                 <div class="modularSystemRequirement"><a data-toggle="modal" data-target="#systemRequirementInfo">{{ node.field_system_requirement_title.value }}</a></div>
                  {% if node.field_system_requirement_content.value is not empty %}
                    <div id="systemRequirementInfo" class="modal fade" role="dialog">
                      <div class="modal-dialog">
                        <!-- Modal content-->
                        <div class="modal-content">
                          <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal">&times;</button>
                            <h4 class="modal-title">{{node.field_system_requirement_title.value}}</h4>
                          </div>
                          <div class="modal-body">
                            {{ node.field_system_requirement_content.value|raw }}
                          </div>
                        </div>
                      </div>
                    </div>
                  {% endif %}
                {% endif %}
              {# System Requirement  End #}
            </div>
            {% if node.field_result_pdf.entity.fileuri is not empty %}
              <div class="col-md-12 col-md-12 col-xs-12 result-pdf-para">
                <span>{{ node.field_result_pdf.description }}</span>
                <a class="btn btn-res" target="_blank" href="{{ file_url(node.field_result_pdf.entity.fileuri) }}">View Result</a>
              </div>
            {% endif %}
          </div>
        </div>
        

        <div class="col-md-3 col-md-offset-0 newEmdedFormWidth mobile-hide">        
            <div class="row">
            {# {{ multistep_page_block_form|raw }} #}  
                <div id="multi-div" class="iciciCheckEligiblityForm"></div>   
            </div>
        </div>
  </div>
</div>
</section>
</section>

