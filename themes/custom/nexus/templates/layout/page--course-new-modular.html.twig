<!-- Rating and Review Section start -->
{% if genarateRatingDataCount.totalRating == true %}
<div id="reviewsRatingsSectionNav"></div>
<section class="ratingSection rating-mt-4" id="ratingSection">
  <div class="container">
    <h2 class="iciciMainTitle mb-5">{{ node.field_rating_and_reviews_title.value }}</h2>
    <div class="row rating-border-top rating-border-bottom bg-white">
      <div class="col-md-4 rating-border-left">
        <h3 class="rating-mt-0 rating-mb-0 rating-title">{{ node.field_rating_title.value }}</h3>
      </div>
      <div class="col-md-8 rating-border-left mobile-hide rating-border-right">
        <h3 class="rating-mt-0 rating-title rating-mb-0">{{ node.field_reviews_title.value }}</h3>
      </div>
      {# <div class="col-md-4 rating-border-right rating-viewBtnCss">
        <a href="#" class="rating-viewBtn">View All</a>
      </div> #}
    </div>
    <div class="row rating-border-bottom bg-white">
