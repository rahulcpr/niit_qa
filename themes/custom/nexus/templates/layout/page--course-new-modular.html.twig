<!-- Header Start-->
<header role="banner">
    {% include directory ~ '/templates/layout/header.html.twig' %}
</header>
<!-- Header End -->
<section id="stackroute-pageMainSection" class="career-pagemainsection modular-pagemainsection">
<!-- Breadcrub Start -->
<section class="course-page-breadcrub mobile-hide icici-breadcrumb">
  <div class="container">
    <div class="row">
            <div class="col-lg-12">
                <div class="page-breadcrumbs">
                    <ul class="breadcrumb">
                        <li>
                            <a href="/">Home</a>
                        </li>
                        <li>
                            <a id="courseCategory"
                                href="{{ path('entity.node.canonical', {'node':  node.field_top_course_category.entity.id}) }}">{{ node.field_top_course_category[0].entity.title.value }}
                            </a> 
                        </li>
                        <li>{{ node.label }}</li> 
                    </ul>
                </div>
            </div>
      </div>
  </div>
</section>
<!-- Breadcrub End -->

<!-- nav bar stricky Start-->
<div id="icici-page-navbar">
  <div id="icici-navbar">
    <div class="container">
      <div class="row desktop-hide icici-page-scroll">
        <ul class="nav nav-tabs" role="tablist">
          {% if node.field_floater_menu is not empty %}
            {% for key, value in node.field_floater_menu %}
              <li><a href="#{{ value.entity.field_menu_section.value }}" class="searchbycharICICI">{{ value.entity.field_menu_title.value }}</a></li>
            {% endfor %}
          {% endif %}
          {% if node.field_click_to_call_phone_number.value is not empty %}
            <li><a href="tel:{{ node.field_click_to_call_phone_number.value }}"><i class="fa fa-phone"></i>{{ node.field_click_to_call_phone_number.value }}</a></li>
          {% endif %}
          <li>{# {{ modular_enrollnow|raw }} #}
			 <div class="enrollnow-div"></div>
		   </li>
        </ul>
      </div>
      <div class="row pl-0 mobile-hide">
        <div class="col-md-8 icici-page-scroll">
          {% if node.field_floater_menu is not empty %}
            {% for key, value in node.field_floater_menu %}
              <a href="#{{ value.entity.field_menu_section.value }}" class="searchbycharICICI">{{ value.entity.field_menu_title.value }}</a>
            {% endfor %}
          {% endif %}
        </div>
        <div class="col-md-4">
          <p class="iciciStickyNav">
            {% if node.field_click_to_call_phone_number.value is not empty %}
              <a href="tel:{{ node.field_click_to_call_phone_number.value }}"><i class="fa fa-phone"></i>{{ node.field_click_to_call_phone_number.value }}</a>
            {% endif %}
            {# {{ modular_enrollnow|raw }} #}
			 <div class="enrollnow-div"></div></p>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- nav bar stricky end-->

<!-- Upper Banner start-->
<section class="newImgBanner stackRouteNewBanner">
  <div class="newBannerImage">
    {% if node.field_background_video_image.entity.fileuri is not empty %}
      <picture>
          <source class="img-responsive banner-img js-lazyload-image" data-srcset="{{ webp_image_get(node.field_background_video_image.entity.fileuri |image_style('1000')) }}" type="image/webp" srcset="/india/sites/default/files/transparentimg.jpg" alt="{{ node.field_background_video_image.alt  }}">
          <img class="img-responsive banner-img js-lazyload-image" data-src="{{ node.field_background_video_image.entity.fileuri }}" src="/india/sites/default/files/transparentimg.jpg" alt="{{ node.field_background_video_image.alt  }}">
        </picture>
    {% endif %}
  </div>
  <section class="newBannerStyle">
    <div class="container">
      <div class="newBannerTextOverlay">
        <div class="col-md-9 pl-0 pt-5 pr-0">
          <div class="newIciciLogo">
              {% if node.field_title_logo.entity.fileuri is not empty %}
                <picture>
                  <source class="img-responsive iciciLogo js-lazyload-image" data-srcset="{{ webp_image_get(node.field_title_logo.entity.fileuri |image_style('1000')) }}" type="image/webp" srcset="/india/sites/default/files/transparentimg.jpg" alt="{{ node.field_title_logo.alt }}"/>
                  <img class="img-responsive iciciLogo js-lazyload-image" data-src="{{ node.field_title_logo.entity.fileuri }}" src="/india/sites/default/files/transparentimg.jpg" alt="{{ node.field_title_logo.alt }}"/>
                </picture>
                {% endif %}
            </div>

            <div class="offer_ticker mt-5">
              {% if node.field_placement_offer.value is not empty %}
              {% if node.field_placement_offer_text.value is not empty %}
                <span class="ticker_title">{{ node.field_placement_offer.value }}</span>
                <div class="programme_ticker">
                <div class="ticker_text_animate"><marquee>{{ node.field_placement_offer_text.value }}</marquee></div>
                </div>
                {% endif %}
                {% endif %}
              </div>

          <div class="newCourseDetail-1">
            <h1 class="mt-2">{{ node.title.value }}</h1>
              {% if node.field_course_summary.value is not empty %}
                <h6>{{ node.field_course_summary.value }}</h6>
              {% endif %}
            <h5 class="newRating">
              {% if CourseStarRating is not empty %}
                {{ CourseStarRating|raw }}
              {% endif %}
              {% if CourseReview is not empty %}
                <span>{{ CourseReview }}</span>
              {% endif %}
            </h5>
            <p class="vide0Card-share-icons share-icici new-share-icon-modular ">Share
              <a target="_blank" href="https://www.facebook.com/sharer/sharer.php?u={{ request_protocol_domain ~ request_path_uri }}" rel="nofollow"><i class="fa fa-facebook"></i></a>

              <a target="_blank" href="https://twitter.com/intent/tweet?url={{ request_protocol_domain ~ request_path_uri }}" rel="nofollow"><i class="fa fa-twitter"></i></a>

              <a target="_blank"  href="https://www.linkedin.com/cws/share?url={{ request_protocol_domain ~ request_path_uri }}&token=&isFramed=true" rel="nofollow"><i class="fa fa-linkedin"></i></a>

            </p>
            <span>
              {% if node.field_course_video_review[0].input is not empty %}
              <p class="col-md-4 col-xs-6 newIntroVideo" onclick="video_iframe_popup('{{ node.field_course_video_review[0].input }}')">
                <img data-src="/india/themes/custom/nexus/assets/images/video-icon.png" class="img-responsive js-lazyload-image" src="/india/sites/default/files/transparentimg.jpg" alt="NIIT program video">
                <span>Watch <br><span>INTRO VIDEO</span></span>
              </p>
              {% endif %} 
            </span>
            <div class="col-md-3 col-xs-6 pl-0 pr-0">
                {% if node.field_mode_of_delivery.value is not empty %} 
                <div class="modeOfdelivery">
                  <h5>Mode Of Delivery</h5>
                  <p class="mb-0">{{node.field_mode_of_delivery.value}}
                    {% if node.field_mode_of_delivery_tooltip.value is not empty %}
                      <small class="FeeTooltip deliveryModeInfo">
                        <i class="fa fa-info-circle"></i>
                        <small class="tip_popup">{{ node.field_mode_of_delivery_tooltip.value }}</small>
                    </small>
                    {% endif %}
                  </p>
                </div>
                {% endif %}
                {# System Requirement  Start #}
				{% if node.field_system_requirement_title.value is not empty %}
					<div class="modularSystemRequirement"><a data-toggle="modal" data-target="#systemRequirementInfo">{{ node.field_system_requirement_title.value }}</a></div>
					{% if node.field_system_requirement_content.value is not empty %}
						<div id="systemRequirementInfo" class="modal fade" role="dialog">
	                        <div class="modal-dialog">
	                          <!-- Modal content-->
	                          <div class="modal-content">
	                            <div class="modal-header">
	                              <button type="button" class="close" data-dismiss="modal">&times;</button>
	                              <h4 class="modal-title">{{node.field_system_requirement_title.value}}</h4>
	                            </div>
	                            <div class="modal-body">
	                              {{ node.field_system_requirement_content.value|raw }}
	                            </div>
	                          </div>
	                        </div>
	                    </div>
	                {% endif %}
	            {% endif %}
	            {# System Requirement  End #}
              </div>
              {# <span class="desktop-hide">
                {% if node.field_course_video_review[0].input is not empty %}
                <p class="col-md-4 newIntroVideo" data-toggle="modal" data-target="#myModal-videos">
                  <img data-src="/india/themes/custom/nexus/assets/images/video-icon.png" class="img-responsive js-lazyload-image" src="/india/sites/default/files/transparentimg.jpg">
                  <span>Watch <br><span>INTRO VIDEO</span></span>
                </p>
                {% endif %} 
              </span> #}

          </div>
        </div>
        <div class="col-md-3 col-md-offset-0 modular-right-banner mobile-hide">
          <div class="priceBoxModular">
            <div class="col-md-6 rup-price">
              <h3 class="actPrice">
              <i class="fa fa-rupee"></i> {{ maincoursebatchfee|raw }}
            </h3> 
            </div>
            <div class="col-md-6">
              {% if maincoursebasefeecheck is not empty %}
                <h5  class="DelPrice"><del>
                  <i class="fa fa-rupee"></i> {{ maincoursebasefee|raw }}
                  </del>
                </h5>
              {% endif %} 
            </div>
            <div class="col-md-12 gst-small">
              <small class="ex-gst">( Excluding GST @ 18% )</small>
            </div>
            

            <div class="col-md-12 mt-3">
              {% if CoursebatchStartTime is not empty %}
                <h5 class="batchSelct">Batch Starting From {{ CoursebatchStartTime|raw }}</h5>
              {% endif %}
              {% if CoursebatchTimings is not empty %}
                <div class="alert alert-info priceBoxModularBatch">{{ CoursebatchTimings|raw }} 
                  <span class="badgeImg2">
                    <img data-src="/india/themes/custom/nexus/assets/images/check-circle.png" class="img-responsive js-lazyload-image" src="/india/sites/default/files/transparentimg.jpg" align="check">
                  </span>
                </div>
              {% endif %}
            </div>
            {% if node.field_additional_off is not empty %}
              <div class="flag"><p class="flag__text">{{ node.field_additional_off.value }}</p></div>
            {% endif %}

           <div class="col-md-12 mt-20px">
             {# {{ modular_enrollnow|raw }} #}
			 <div class="enrollnow-div"></div>
           </div>
           <div class="col-md-12">
             <p class="stillText">Still confused?{{ requestACallBackFormBtn|raw }}</p>
           </div>

         </div>          
        </div>
      </div>
    </div>
  </section>
</section>
<!-- Upper Banner End -->

