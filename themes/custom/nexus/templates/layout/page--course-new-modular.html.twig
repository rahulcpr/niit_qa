<!-- Program Overview Start -->
<div id="programOverviewSectionNav"></div>
<section id="iciciPageNavbar" class="pt-3 mb-4 CourseBgColorProgramOverview">
  {% if node.field_program_overview_image.entity.fileuri is not empty %}
    <h2 class="iciciMainTitle mb-5">{{ node.field_program_overview_image.title  }}</h2>
  {% endif %}
    <div class="iciciOverviweleftImg">
      {% if node.field_program_overview_image.entity.fileuri is not empty %}
        <picture>
          <source class="img-responsive js-lazyload-image" data-srcset="{{ webp_image_get(node.field_program_overview_image.entity.fileuri |image_style('1000')) }}" type="image/webp" srcset="/india/sites/default/files/transparentimg.jpg" alt="{{ node.field_program_overview_image.alt  }}"/>
          <img class="img-responsive js-lazyload-image" data-src="{{ node.field_program_overview_image.entity.fileuri }}" src="/india/sites/default/files/transparentimg.jpg" alt="{{ node.field_program_overview_image.alt  }}"/>
        </picture>
        {% endif %}
        </div>
        <div class="container">
            <div class="row">
                <div class="col-md-6 col-md-offset-5 icicOverviewBg">
                    <div class="overViewTextRight">
                      {{node.body.value|raw}}
                    </div>
                </div>      
            </div>
        </div>   
</section> 
<!-- Program Overview End -->

<!-- CourseDetail Section Start -->
<div id="courseDetailsSectionNav"></div>
<section id="iciciCourseDetail"  class="careercoursedetails pt-3 mb-4">
  <div class="container">
    <h2 class="iciciMainTitle mb-5">{{ node.field_course_details_title.value }}</h2>
    <div class="row">
      <div class="col-md-12">
        <div class="col-md-9 pl-0">
          {{ node.field_course_details.value|raw }}
        </div>

        <div class="col-md-3 brochureDwnloadBtn-parent">
          {% if node.field_course_brochure.entity.fileuri is not empty %}
            <a class="btn brochureDwnloadBtn" target="_blank" href="{{ file_url(node.field_course_brochure.entity.fileuri) }}" download><i class="fa fa-download"></i> Download Brochure</a>
          {% endif %}
        </div>
      </div>
            
      {% if node.field_course_module_faq is not empty %}
      <div class="col-md-12 mt-3 iciciAccordianDetail">
        <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
          {% for key, data in node.field_course_module_faq %}
            <div class="panel panel-default">
              <div class="panel-heading" role="tab" id="headingTwo">
                <h4 class="panel-title">
                  <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapse-course-{{ key }}" aria-expanded="false" aria-controls="collapseOne">
                    {{ data.question }}
                  </a>
                </h4>
              </div>
              <div id="collapse-course-{{ key }}" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">
                <div class="panel-body">
                  {{ data.answer|raw }}
                </div>
              </div>
            </div>
          {% endfor %}  
        </div>
      </div>
      {% endif %}
    </div>
  </div>
</section>    
<!-- CourseDetail Section end -->

