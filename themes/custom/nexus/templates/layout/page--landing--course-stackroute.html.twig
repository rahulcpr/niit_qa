<div id="mynodeid">{{nodeidfortemplate}}</div>
<!-- Header End -->
<section id="stackroute-pageMainSection" class="stackroute-pageMainSection">

<!-- nav bar stricky Start-->
<div id="icici-page-navbar">
  <div id="icici-navbar">
    <div class="container">
      <div class="row desktop-hide icici-page-scroll">
        <ul class="nav nav-tabs" role="tablist">
          {% if node.field_floating_menu is not empty %}
            {% for key, value in node.field_floating_menu %}
              <li><a href="#{{ value.entity.field_menu_section.value }}" class="searchbycharICICI">{{ value.entity.field_menu_title.value }}</a></li>
            {% endfor %}
          {% endif %}
          {% if node.field_click_to_call_phone_number.value is not empty %}
            <li><a href="tel:{{ node.field_click_to_call_phone_number.value }}"><i class="fa fa-phone"></i>{{ node.field_click_to_call_phone_number.value }}</a></li>
          {% endif %}
          <li>{{ mutlistep_apply_btn|raw }} </li>
        </ul>
      </div>
      <div class="row pl-0 mobile-hide">
        <div class="col-md-8 icici-page-scroll">
          {% if node.field_floating_menu is not empty %}
            {% for key, value in node.field_floating_menu %}
              <a href="#{{ value.entity.field_menu_section.value }}" class="searchbycharICICI">{{ value.entity.field_menu_title.value }}</a>
            {% endfor %}
          {% endif %}
        </div>
        <div class="col-md-4">
          <p class="iciciStickyNav">
            {% if node.field_click_to_call_phone_number.value is not empty %}
              <a href="tel:{{ node.field_click_to_call_phone_number.value }}"><i class="fa fa-phone"></i>{{ node.field_click_to_call_phone_number.value }}</a>
            {% endif %}
            {{ mutlistep_apply_btn|raw }} </p>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- nav bar stricky end-->

<!-- Course Overview start-->

<section class="newImgBanner stackRouteNewBanner stackRoutelandingpage">
  <div class="newBannerImage">
    {% if node.field_banner.entity.fileuri is not empty %}
      <img class="img-responsive banner-img mobile-hide js-lazyload-image" src="/india/sites/default/files/transparentimg.jpg" data-src = "{{ file_url(node.field_banner.entity.fileuri) }}" alt="{{ node.field_banner.entity.alt  }}" />
    {% endif %}
  </div>
  <section class="newBannerStyle">
    <div class="container">
      <div class="newBannerTextOverlay">
        <div class="col-md-7 pl-0 pr-0">
          <div class="newIciciLogo">
              {% if node.field_related_course_reference.entity.field_title_logo.entity.fileuri is not empty %}
                <img class="img-responsive iciciLogo js-lazyload-image" src="/india/sites/default/files/transparentimg.jpg" data-src = "{{ file_url(node.field_related_course_reference.entity.field_title_logo.entity.fileuri) }}" alt="{{ node.field_related_course_reference.entity.field_title_logo.alt  }}" />
              {% endif %}
            </div>
          <div class="newCourseDetail-1">
            <h1>{{ node.title.value }}</h1>
              {% if node.field_related_course_reference.entity.field_course_summary.value is not empty %}
                <h6>{{ node.field_related_course_reference.entity.field_course_summary.value }}</h6>
              {% endif %}
            {# <h5 class="newRating">
              {% if CourseStarRating is not empty %}
                {{ CourseStarRating|raw }}
              {% endif %}
              {% if CourseReview is not empty %}
                <span>{{ CourseReview }}</span>
              {% endif %}
            </h5> #}
            <p class="vide0Card-share-icons share-icici new-share-icon-modular">Share
              <a target="_blank" href="https://www.facebook.com/sharer/sharer.php?u={{ request_protocol_domain ~ request_path_uri }}" rel="nofollow"><i class="fa fa-facebook"></i></a>

              <a target="_blank" href="https://twitter.com/intent/tweet?url={{ request_protocol_domain ~ request_path_uri }}" rel="nofollow"><i class="fa fa-twitter"></i></a>

              <a target="_blank"  href="https://www.linkedin.com/cws/share?url={{ request_protocol_domain ~ request_path_uri }}&token=&isFramed=true" rel="nofollow"><i class="fa fa-linkedin"></i></a>

            </p>
            <span>
              {% if node.field_related_course_reference.entity.field_course_video_review[0].input is not empty %}
              <p class="col-md-4 col-xs-6 newIntroVideo" onclick="video_iframe_popup('{{ node.field_course_video_review[0].input }}')">
                <img data-src="/india/themes/custom/nexus/assets/images/video-icon.png" class="img-responsive js-lazyload-image" src="/india/sites/default/files/transparentimg.jpg" alt="NIIT program video">
                <span>Watch <br><span>INTRO VIDEO</span></span>
              </p>
              {% endif %} 
            </span>
            <div class="col-md-5 col-xs-6 pl-0 pr-0">
                {% if node.field_related_course_reference.entity.field_mode_of_delivery.value is not empty %} 
                <div class="modeOfdelivery">
                  <h5>Mode Of Delivery</h5>
                  <p>{{node.field_related_course_reference.entity.field_mode_of_delivery.value}}
                    {% if node.field_related_course_reference.entity.field_mode_of_delivery_tooltip.value is not empty %}
                      <small class="FeeTooltip deliveryModeInfo">
                        <i class="fa fa-info-circle"></i>
                        <small class="tip_popup">{{ node.field_related_course_reference.entity.field_mode_of_delivery_tooltip.value }}</small>
                    </small>
                    {% endif %}
                  </p>
                </div>
                {% endif %}
              </div>
          </div>
        </div>

        <div class="col-md-3 col-md-offset-2 newEmdedFormWidth stackrouteEnrollForm strouteEnrollFormSec mobile-hide">
          <div class="row">
             {# {{ multistep_page_block_form|raw }} #}
             <div id="multi-div" class="iciciCheckEligiblityForm"></div>  
          </div>
        </div>
      </div>
    </div>
  </section>
</section>


