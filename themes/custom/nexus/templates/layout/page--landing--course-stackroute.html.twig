      <div class="lerning-boxes-style">
          <img data-src="{{ file_url(value.entity.field_image_pedagogy.entity.fileuri) }}" alt="{{ value.entity.field_image_pedagogy.alt }}" class="img-responsive js-lazyload-image" src="/india/sites/default/files/transparentimg.jpg">
          <h3>{{ value.entity.field_title_pedagogy.value }}</h3>
          <p>{{ value.entity.field_summary_pedagogy.value }}</p>
      </div>
    </div>
    {% endfor %}
  </div>
</section>
{% endif %}
