            <span>Watch <br><span class="blue-color">VIDEO</span></span>
            <img data-src="/india/themes/custom/nexus/assets/images/video-icon.png" class="img-responsive js-lazyload-image" src="/india/sites/default/files/transparentimg.jpg" alt="NIIT program video">
          </p>
        {% endif %} 
      </div>
    </div>
  </div>
</section>
{% if node.field_related_course_reference.entity.field_pedagogy_image_title_summa is not empty %}
<section id="pedagogySecGrid" class="pedagogySecGrid CourseBgColorwhite pt-5">
  <div class="container">
    {% for key, value in node.field_related_course_reference.entity.field_pedagogy_image_title_summa %}
