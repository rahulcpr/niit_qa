              {% endfor %} 
            </ul>
          {% endif %}
        </div>
      </div>
      <div class="col-md-2 col-xs-6 col-xs-offset-3 col-md-offset-0">
        {% if node.field_related_course_reference.entity.field_pedagogy_video.value is not empty %}
