<!-- Programme Overview Start-->
<div id="programOverviewSectionNav"></div>
<section class="programSummary CourseBgColorwhite pb-5">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <h2 class="newTitleCsss">{{ node.field_related_course_reference.entity.field_program_overview_title_cou.value  }}</h2>
        <h5 class="newSubTitleCss">
          {% if node.field_related_course_reference.entity.body.value is not empty %}
              {{node.field_related_course_reference.entity.body.value|raw}}
          {% endif %}
        </h5>

        <div class="row">
          {% if node.field_related_course_reference.entity.field_program_overview_icon_text is not empty %}
            {% for key, item in node.field_related_course_reference.entity.field_program_overview_icon_text %}
               <div class="col-md-4">
                <div class="newKeyHighlightListing">
                  <img data-src="{{ file_url(item.entity.field_icon.entity.fileuri) }}" class="img-responsive js-lazyload-image" src="/india/sites/default/files/transparentimg.jpg" alt="{{ item.field_icon.alt }}">
                  <span>{{ item.entity.field_text.value }}</span>
                </div>
              </div>
            {% endfor %}  
           
          {% endif %}
        </div>

      </div>
    </div>
  </div>
</section>
