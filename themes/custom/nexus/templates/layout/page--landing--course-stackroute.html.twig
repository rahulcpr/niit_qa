<!-- Our Pedagogy Start -->
<div id="ourPedagogySectionNav"></div>
<section id="pedagogySec" class="ourPedagogy pt-3 pb-3 mb-0 mt-5">
  <div class="container">
    <div class="row">
      <h2 class="newTitleCsss text-white pt-0">{{ node.field_related_course_reference.entity.field_pedagogy_title.value }}</h2>
