<!-- Header Start-->
<header role="banner">
    {% include directory ~ '/templates/layout/header.html.twig' %}
</header>
<!-- Header End -->



{% if node.field_instruction_banner_image.entity.fileuri is not empty %}
  <section class="student-signin-page-bg niitDigitalBanner mobile-hide">
        <img data-src="{{ file_url(node.field_instruction_banner_image.entity.fileuri) }}" class="img-responsive js-lazyload-image" src="/india/sites/default/files/transparentimg.jpg"  alt="{{ node.field_instruction_banner_image.alt  }}">
  </section>
{% endif %}

{% if node.field_mobile_banner_image1.entity.fileuri is not empty %}
  <section class="student-signin-page-bg niitDigitalBanner desktop-hide">
        <img data-src="{{ file_url(node.field_mobile_banner_image1.entity.fileuri) }}" class="img-responsive js-lazyload-image" src="/india/sites/default/files/transparentimg.jpg"  alt="{{ node.field_mobile_banner_image1.alt  }}">
  </section>
{% endif %}


<section class="learning-app-webpage">
  <div class="container">
    <h2 class="learning-app-webpage-title">{{ node.field_instruction_title.value }}</h2>
    <p class="learning-app-webpage-Subtitle">{{ node.field_instruction_summary.value }}</p>
    <div class="row">
      <div class="col-md-5 col-sm-5">
        <div class="learning-app-webpage-imageIntrction">
          {% if node.field_instruction_image.entity.fileuri is not empty %}
            <section class="student-signin-page-bg">
                  <img data-src="{{ file_url(node.field_instruction_image.entity.fileuri) }}" class="img-responsive js-lazyload-image" src="/india/sites/default/files/transparentimg.jpg"  alt="{{ node.field_instruction_image.alt  }}">
            </section>
          {% endif %}
        </div>
      </div>
      <div class="col-md-7 col-sm-7">
        <div class="webpage-instr-list">
          {{ node.body.value|raw }}
        </div>
      </div>
    </div>
  </div>
</section>

{% if node.field_message.value is not empty %}
<section class="programeSchedule">
  <div class="container">
    {# <h2 class="learning-app-webpage-title">programme schedule</h2> #}
    <div class="row">
      <div class="col-md-8 col-md-offset-2">

        <div class="row marginTop">
          <div class="col-md-12">
            <div class="alert alert-info">
               {{ node.field_message.value }}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
{% endif %}


{% if node.field_video_url.value is not empty %}
<section class="learning-app-webpage-videoSec">
  <div class="container">
    <h2 class="learning-app-webpage-title">{{ node.field_video_title.value }}</h2>
    <div class="row">
      <div class="col-md-8 col-md-offset-2">
         <div class="embed-ifram">
           <iframe width="100%" height="420" src="{{ node.field_video_url.value }}" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
          </div>
      </div>
    </div>
  </div>
</section>
{% endif %}

<!--FAQ start here -------->
<section id="Icicifaq-sec" class="icici-faq stackroutFAQ niitDigitalFAQNew">
   <div class="container">
    <h2 class="learning-app-webpage-title">{{ node.field_faq_title_section.value }}</h2>
      <p class="learning-app-webpage-Subtitle">{{ node.field_faq_summary_section.value }}</p>
      {% if node.field_faq_section is not empty %}
      <div class="col-md-12 mt-3 iciciAccordianDetail">
        <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
          {% for key, data in node.field_faq_section %}
            <div class="panel panel-default">
              <div class="panel-heading" role="tab" id="headingTwo">
                <h4 class="panel-title">
                  <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapse-{{ key }}" aria-expanded="false" aria-controls="collapseOne">
                    {{ data.question }}
                  </a>
                </h4>
              </div>
              <div id="collapse-{{ key }}" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">
                <div class="panel-body">
                  {{ data.answer|raw }}
                </div>
              </div>
            </div>
          {% endfor %}  
        </div>
      </div>
      {% endif %}
   </div>
</section>
<!--FAQ end here -------->




